{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",
  "description": "Description of my extension",
  "icons": [
    {
      "src": "favicon.ico",
      "48": "favicon.ico",
      "128": "meet-app-128.png"
    }
  ],
  "browser_action": {
    "default_icon": "meet-app-48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "images/*.png"
  ]
}
